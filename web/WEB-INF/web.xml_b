<?xml version="1.0"?>
<web-app>
    <display-name>Prodigy Application</display-name>
    <servlet>
        <servlet-name>Rest</servlet-name>
        <servlet-class>
            com.manzuorla.prodigy.Prodigy
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Rest</servlet-name>
        <url-pattern>/rest/*</url-pattern>
    </servlet-mapping>
</web-app>